{
  "compilerOptions": {
    "target": "esnext",
    "module": "es2015",
    "jsx": "react-native",
    "allowJs": true,
    "strict": true,
    "outDir": "./.ts-compiled",
    "rootDir": "./src",
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "types": []
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules",
  ]
}
